{
    "manifest_version": 3,
    "name": "Dynamic Data Form Filler",
    "version": "1.0",
    "description": "A Chrome extension to fill form fields with dynamic data fetched from a Puppeteer server.",
    "permissions": [
      "activeTab",
      "scripting"
    ],
    "host_permissions": [
      "http://*/*",
      "https://*/*"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html"
     
    }
   
  }
  